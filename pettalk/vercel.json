{
  "version": 2,
  "builds": [
    {
      "src": "src/pages/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/**/*.js",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_APPLICATION_CREDENTIALS_JSON": "@google_application_credentials_json"
  }
}